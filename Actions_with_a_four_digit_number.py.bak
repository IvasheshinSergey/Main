def checkTriangleSides():
    print("Проверяем возможность составления треугольника из введенных сторон")
    side_1 = int(input("Введите длинну первой стороны:"))
    side_2 = int(input("Введите длинну второй стороны:"))
    side_3 = int(input("Введите длинну третьей стороны:"))
    if side_1 < side_2 + side_3 :
        if side_2 < side_1 + side_3 :
            if side_3 < side_1 + side_2 :
                print("Из этого можно составить треугольник")
            else:
                print("треть сторона слишком большая")
        else:
            print("вторая сторона слишком большая")
    else:
        print("первая сторона слишком большая")